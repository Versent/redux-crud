# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 2

[*.ts]
indent_style = tab
indent_size = 2

[*.js]
indent_style = tab
indent_size = 4

[*.json]
indent_style = tab
indent_size = 4

[Makefile]
indent_style = tab
